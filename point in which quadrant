#include <iostream>
using namespace std;

int main(){
    int x, y;
    cout<<"enter x value: \n";
    cin>>x;
    cout<<"enter y value: \n";
    cin>>y;

    if (x>0 && y>0) {
        cout<<"Your point ("<<x<<","<<y<<") lies in first quadrant";
    }
    else if ( x<0 && y>0 ){
        cout<<"Your point ("<<x<<","<<y<<") lies in second quadrant";
    }
    else if ( x<0 && y<0 ){
        cout<<"Your point ("<<x<<","<<y<<") lies in third quadrant";
    }
    else if ( x>0 && y<0 ){
        cout<<"Your point ("<<x<<","<<y<<") lies in fourth quadrant";
    }
    else {
        cout<<"your point is on origin";
    }
}
